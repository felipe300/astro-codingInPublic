{
  "$schema": "https://frontmatter.codes/frontmatter.schema.json",
  "frontMatter.framework.startCommand": "pnpm run dev",
  "frontMatter.preview.host": "http://localhost:3000",
  "frontMatter.taxonomy.contentTypes": [
    {
      "name": "default",
      "pageBundle": false,
      "previewPath": "'blog'",
      "fields": [
        {
          "title": "title",
          "name": "title",
          "type": "string"
        },
        {
          "title": "date",
          "name": "date",
          "type": "datetime",
          "default": "{{now}}"
        },
        {
          "title": "author",
          "name": "author",
          "type": "string",
          "taxonomyLimit": 1
        },
        {
          "title": "image",
          "name": "image",
          "type": "image",
          "isPreviewImage": true
        },
        {
          "title": "description",
          "name": "description",
          "type": "string"
        },
        {
          "title": "draft",
          "name": "draft",
          "type": "boolean"
        },
        {
          "title": "category",
          "name": "category",
          "type": "category",
          "taxonomyLimit": 1
        }
      ]
    }
  ],
  "frontMatter.framework.id": "other",
  "frontMatter.content.publicFolder": "public",
  "frontMatter.content.pageFolders": [
    {
      "title": "blog",
      "path": "[[workspace]]/src/content/blog",
      "contentTypes": [
        "default"
      ]
    }
  ],
  "frontMatter.taxonomy.tags": [
    "CSS",
    "Reference Docs",
    "Astro",
    "General"
  ],
  "frontMatter.taxonomy.author": [
    "Anna Dixon",
    "Victoria Greenfelder",
    "Darnell McClure"
  ]
}