{
  "extends": "astro/tsconfigs/strict",
  // type alias
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@components/*": [
        "src/components/*"
      ],
      "@layouts/*": [
        "src/layouts/*"
      ],
      "@styles/*": [
        "src/styles/*"
      ],
      "@types": [
        "src/types"
      ],
      "@utils": [
        "src/utils"
      ],
    }
  }
}